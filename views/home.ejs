<%- include("partials/header"); -%>

<section class="container-fluid">
  <h1>Home</h1>
  <p><%= HOMECONTENT %></p>
  <% POSTS.forEach(function(post) { %>
  <div>
    <h2><%= post.title %></h2>
    <p>
      <%= (post.content).substring(0,100); %>..
      <a href="/posts/<%=post._id%>">Read More</a>
    </p>
  </div>
  <% }); %>
</section>

<%- include("partials/footer"); -%>